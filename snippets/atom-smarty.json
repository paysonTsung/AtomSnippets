{
    "print-data": {
        "prefix": "cprint",
        "body": [
            "<script type=\"text/javascript\">",
            "    console.log({%json_encode(\\$tplData)%});",
            "</script>"
        ],
        "description": "print-data"
    },
    "$tplData": {
        "prefix": "data",
        "body": [
            "\\$tplData"
        ],
        "description": "tpl-data"
    },
    "coat": {
        "prefix": "ll",
        "body": [
            "{%$0%}"
        ],
        "description": "coat"
    },
    "if": {
        "prefix": "cif",
        "body": [
            "{%if $1%}",
            "    $0",
            "{%/if%}"
        ],
        "description": "if"
    },
    "if-else": {
        "prefix": "cifelse",
        "body": [
            "{%if $1%}",
            "    $2",
            "{%else%}",
            "    $3",
            "{%/if%}"
        ],
        "description": "ifelse"
    },
    "if-no-empty": {
        "prefix": "ifempty",
        "body": [
            "{%if !empty($1)%}",
            "    $0",
            "{%/if%}"
        ],
        "description": "if-no-empty"
    },
    "if-no-isset": {
        "prefix": "ifisset",
        "body": [
            "{%if isset($1)%}",
            "    $0",
            "{%/if%}"
        ],
        "description": "if-isset"
    },
    "foreach": {
        "prefix": "cforeach",
        "body": [
            "{%foreach $1 as ${3:\\$index}=>${2:\\$item}%}",
            "    $0",
            "{%/foreach%}"
        ],
        "description": "foreach"
    },
    "empty": {
        "prefix": "empty",
        "body": [
            "empty($0)"
        ],
        "description": "empty"
    },
    "isset": {
        "prefix": "isset",
        "body": [
            "isset($0)"
        ],
        "description": "isset"
    },
    "tc-link": {
        "prefix": "tclink",
        "body": [
            "{%fe_fn_atom_tclink url=\\$tplData.${1:tcUrl} noreturn=1%}",
            "{%\\$tplData.${0:tcParams} = \\$__tc_params%}"
        ],
        "description": "tclink"
    },
    "sf-link": {
        "prefix": "sflink",
        "body": [
            "{%fe_fn_atom_sflink url=\\$tplData.${1:sfUrl}%}",
            "{%\\$tplData.${0:sfParams} = \\$__sf_params%}"
        ],
        "description": "sflink"
    },
    "mip-link": {
        "prefix": "miplink",
        "body": [
            "{%fe_fn_atom_miplink url=\\$tplData.${1:mipUrl} title=\\$tplData.${2:mipTitle}%}",
            "{%\\$tplData.${0:mipParams} = \\$__mip_params%}"
        ],
        "description": "miplink"
    },
    "mip-link-nohead": {
        "prefix": "miplink-nohead",
        "body": [
            "{%fe_fn_atom_miplink url=\\$tplData.${1:mipUrl} title=\\$tplData.${2:mipTitle}%}",
            "{%\\$tplData.${0:mipParams} = \\$__mip_params%}",
            "{%\\$tplData.${0:mipParams}.sfUrl = \\$__mip_params.sfUrl|cat:'?nohead=1'%}"
        ],
        "description": "miplink-nohead"
    },
    "vsearch-link": {
        "prefix": "vsearchlink",
        "body": [
            "{%fe_fn_atom_vsearch_url pd=$2 sa=$3 url=\\$tplData.${1:vsearchUrl}%}",
            "{%\\$tplData.${0:vsearchParams} = \\$__vsearch_params%}"
        ],
        "description": "vsearch-link"
    },
    "timg-link": {
        "prefix": "timglink",
        "body": [
            "{%\\$tplData.${1:imgUrl} = {%fe_fn_atom_timg imgsrc=\\$tplData.${2:img}%}%}"
        ],
        "description": "timg-link"
    },
    "tpl-info": {
        "prefix": "tplinfo",
        "body": [
            "{%fe_fn_atom_tpl_info%}",
            "{%\\$tplData.tplInfo = \\$__tpl_info%}"
        ],
        "description": "tpl-info"
    },
    "format-date": {
        "prefix": "formatdate",
        "body": [
            "{%${1:\\$formatedDate} = {%fe_fn_atom_date_format timestamp=$2%}%}"
        ],
        "description": "format-date"
    },
    "time-span": {
        "prefix": "timespan",
        "body": [
            "{%${1:\\$timeSpan} = {%fe_fn_atom_time_span timestamp=$2%}%}"
        ],
        "description": "timespan"
    },
    "highlight": {
        "prefix": "highlight",
        "body": [
            "{%\\$tplData.${0:title} = \\$tplData.${0:title}|@highlight%}"
        ],
        "description": "highlight"
    },
    "https-host": {
        "prefix": "httpshost",
        "body": [
            "{%\\$tplData.${0:imgSrc} = \\$tplData.${0:imgSrc}|getHttpsHost%}"
        ],
        "description": "https-host"
    }
}