{
    "coat": {
        "prefix": "ll",
        "body": [
            "{{ $1 }}"
        ],
        "description": "Atom文本插值"
    },
    "htmlcs-disable": {
        "prefix": "htmlcs-disable",
        "body": [
            "<!-- htmlcs-disable -->"
        ],
        "description": "htmlcs-disable"
    },
    "htmlcs-enable": {
        "prefix": "htmlcs-enable",
        "body": [
            "<!-- htmlcs-enable -->"
        ],
        "description": "htmlcs-enable"
    },
    "component": {
        "prefix": "component",
        "body": [
            "<component",
            "\t:is=\"$1\"",
            ">",
            "</component>"
        ],
        "description": "component组件标签"
    },
    "template": {
        "prefix": "template",
        "body": [
            "<template>",
            "\t$0",
            "</template>"
        ],
        "description": "template模板标签"
    },
    "slot": {
        "prefix": "slot",
        "body": [
            "<slot>",
            "\t$0",
            "</slot>"
        ],
        "description": "slot插槽便签"
    },
    "div": {
        "prefix": "div",
        "body": [
            "<div>",
            "\t$0",
            "</div>"
        ],
        "description": "div标签"
    },
    "span": {
        "prefix": "span",
        "body": [
            "<span>$1</span>"
        ],
        "description": "span标签"
    },
    "filter-sf": {
        "prefix": "|sf",
        "body": [
            "| tc({",
            "    type: 'sf'",
            "})"
        ],
        "description": "filter拼sf"
    },
    "filter-mip": {
        "prefix": "|mip",
        "body": [
            "| tc({",
            "    type: 'mip',",
            "    title: $1",
            "})"
        ],
        "description": "filter拼mip"
    },
    "filter-vsearch": {
        "prefix": "|vsearch",
        "body": [
            "| tc({",
            "    type: 'vsearch'",
            "})"
        ],
        "description": "filter拼vsearch"
    },


    "view": {
        "prefix": "view",
        "body": [
            "<view",
            "\tref=\"${1:container}\"",
            "\t:style=\"$2\"",
            ">",
            "\t$0",
            "</view>"
        ],
        "description": "view"
    },
    "webview": {
        "prefix": "webview",
        "body": [
            "<webview",
            "\tref=\"${1:webview}\"",
            "\tstyle=\"${2:width:100%;height:100%}\"",
            "\thref=\"$3\"",
            "/>"
        ],
        "description": "webview"
    },
    "text": {
        "prefix": "text",
        "body": [
            "<text :style=\"$1\">$0</text>"
        ],
        "description": "text"
    },
    "viewpager": {
        "prefix": "viewpager",
        "body": [
            "<viewpager>",
            "\t$0",
            "</viewpager>"
        ],
        "description": "viewpager"
    },
    "scrollview": {
        "prefix": "scrollview",
        "body": [
            "<scrollview",
            "\tref=\"${1:scrollview}\"",
            "\tstyle=\"$2\"",
            "\t@scroll=\"$3\"",
            ">",
            "\t$0",
            "</scrollview>"
        ],
        "description": "scrollview"
    },
    "image": {
        "prefix": "image",
        "body": [
            "<image height=\"$1\" width=\"$2\" src=\"$3\"></image>"
        ],
        "description": "image"
    },
    "h-scrollview": {
        "prefix": "h-scrollview",
        "body": [
            "<h-scrollview",
            "\tref=\"${1:hScrollview}\"",
            "\tstyle=\"$2\"",
            "\t@scroll=\"$3\"",
            ">",
            "\t$0",
            "</h-scrollview>"
        ],
        "description": "h-scrollview"
    },
    "photo": {
        "prefix": "photo",
        "body": [
            "<photo",
            "\t:src=\"$1\"",
            "\tminimum-scale=\"1\"",
            "\tmedium-scale=\"1.5\"",
            "\tmaximum-scale=\"2\"",
            "\tzoomable=\"${2:false}\"",
            "/>"
        ],
        "description": "photo"
    },
    "video": {
        "prefix": "video",
        "body": [
            "<video",
            "\tref=\"${1:video}\"",
            "\t:src=\"$2\"",
            "\t:title=\"$3\"",
            "\t:controls=\"$4\"",
            "\tstyle=\"$5\"",
            "></video>"
        ],
        "description": "photo"
    },
    "tap": {
        "prefix": "@tap",
        "body": [
            "@tap=\"$1\""
        ],
        "description": "绑定tap事件"
    },
    "focus": {
        "prefix": "@focus",
        "body": [
            "@focus=\"$1\""
        ],
        "description": "绑定focus事件"
    },
    "touchstart": {
        "prefix": "@touchstart",
        "body": [
            "@touchstart=\"$1\""
        ],
        "description": "绑定touchstart事件"
    },
    "touchend": {
        "prefix": "@touchend",
        "body": [
            "@touchend=\"$1\""
        ],
        "description": "绑定touchend事件"
    },
    "touchmove": {
        "prefix": "@touchmove",
        "body": [
            "@touchmove=\"$1\""
        ],
        "description": "绑定touchmove事件"
    },
    "scroll": {
        "prefix": "@scroll",
        "body": [
            "@scroll=\"$1\""
        ],
        "description": "绑定scroll事件"
    }
}