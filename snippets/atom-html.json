{
    "component": {
        "prefix": "/component",
        "body": [
            "<component",
            "\t:is=\"$1\"",
            ">",
            "</component>"
        ]
    },
    "c-aladdin": {
        "prefix": "caladdin",
        "body": [
            "<c-aladdin",
            "\t:url=\"$1\"",
            "\t:title=\"$2\"",
            "\t:show-left-text=\"$3\"",
            "\t:show-right-text=\"$4\"",
            "\t:show-right-url=\"$5\"",
            ">",
            "\t$0",
            "</c-aladdin>"
        ],
        "description": "c-aladdin"
    },
    "c-title": {
        "prefix": "ctitle",
        "body": [
            "<c-title",
            "\t:url=\"$1\"",
            "\t:text=\"$2\"",
            "/>"
        ],
        "description": "c-title"
    },
    "c-line": {
        "prefix": "cline",
        "body": [
            "<c-line clamp=\"$1\"/>",
            "\t$0",
            "</c-line>"
        ],
        "description": "c-line"
    },
    "c-link": {
        "prefix": "clink",
        "body": [
            "<c-link",
            "\t:params=\"$1\"",
            "/>",
            "\t$0",
            "</c-link>"
        ],
        "description": "c-link"
    },
    "c-btn": {
        "prefix": "cbtn",
        "body": [
            "<c-btn",
            "\ttype=\"$1\"",
            "\t:text=\"$2\"",
            "\t:icon=\"$3\"",
            "\t:params=\"$4\"",
            "/>"
        ],
        "description": "c-btn"
    },
    "c-divider": {
        "prefix": "cdivider",
        "body": [
            "<c-divider/>"
        ],
        "description": "c-divider"
    },
    "c-icon": {
        "prefix": "cicon",
        "body": [
            "<c-icon :type=\"$1\"/>"
        ],
        "description": "c-icon"
    },
    "c-img": {
        "prefix": "cimg",
        "body": [
            "<c-img",
            "\t:src=\"$1\"",
            "\t:title=\"$2\"",
            "/>"
        ],
        "description": "c-img"
    },
    "c-label": {
        "prefix": "clabel",
        "body": [
            "<c-label type=\"$1\" :text=\"$2\"/>"
        ],
        "description": "c-label"
    },
    "c-slink": {
        "prefix": "cslink",
        "body": [
            "<c-slink",
            "\ttype=\"$1\"",
            "\t:params=\"$2\"",
            "\t:text=\"$3\"",
            "/>"
        ],
        "description": "c-slink"
    },
    "c-img-content": {
        "prefix": "cimgcontent",
        "body": [
            "<c-img-content",
            "\t:img-src=\"$1\"",
            "\t:img-col=\"${2:2}\"",
            "/>",
            "\t$0",
            "</c-img-content>"
        ],
        "description": "c-img-content"
    },
    "c-list": {
        "prefix": "clist",
        "body": [
            "<c-list>",
            "\t<c-list-item",
            "\t\ta-for=\"(${2:nav}, ${3:index}) in ${1:listArr}\"",
            "\t\t:key=\"$3\"",
            "\t>",
            "\t\t$0",
            "\t</c-list-item>",
            "</c-list>"
        ],
        "description": "c-list"
    },
    "c-navs": {
        "prefix": "cnavs",
        "body": [
            "<c-navs",
            "\t:label-list=\"$1\"",
            "\t@change=\"${2:selectFn}\"",
            "/>"
        ],
        "description": "c-navs"
    },
    "c-tabs": {
        "prefix": "ctabs",
        "body": [
            "<c-tabs",
            "   :selected=\"${1:0}\"",
            "   has-slide-animation",
            "   @change=\"${2:changeFn}\">",
            "   <c-tabs-item",
            "       a-for=\"(${4:tab}, ${5:index}) in ${3:tabs}\"",
            "       :key=\"$5\"",
            "       :label=\"$4.label\"",
            "   >",
            "       $0",
            "   </c-tabs-item>",
            "</c-tabs>"
        ]
    }
}